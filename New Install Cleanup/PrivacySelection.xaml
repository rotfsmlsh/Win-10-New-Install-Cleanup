<Window x:Class="New_Install_Cleanup.PrivacySelection"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:New_Install_Cleanup"
        mc:Ignorable="d"
        Title="Ammonia - Privacy Settings" Height="712" Width="304">
    <Grid>
        <Frame Content="" HorizontalAlignment="Left" Height="661" Margin="10,10,0,0" VerticalAlignment="Top" Width="276" ScrollViewer.VerticalScrollBarVisibility="Auto"/>
        <Expander x:Name="exp_locationSettings" Header="Location Settings" HorizontalAlignment="Left" Height="95" Margin="10,10,0,0" VerticalAlignment="Top" Width="276">
            <Grid Background="#FFE5E5E5">
                <Grid.ColumnDefinitions>
                    <ColumnDefinition Width="69*"/>
                    <ColumnDefinition Width="29*"/>
                </Grid.ColumnDefinitions>
            </Grid>
        </Expander>
        <Expander x:Name="exp_microphoneAccess" Header="Microphone Access" HorizontalAlignment="Left" Height="100" Margin="10,110,0,0" VerticalAlignment="Top" Width="276">
            <Grid Background="#FFE5E5E5"/>
        </Expander>
    </Grid>
</Window>
